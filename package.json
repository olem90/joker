{
  "name": "joker",
  "version": "0.2.2",
  "description": "Application that tells jokes",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olem90/joker.git"
  },
  "keywords": [
    "jokes"
  ],
  "author": "shipit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olem90/joker/issues"
  },
  "homepage": "https://github.com/olem90/joker#readme",
  "devDependencies": {
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1",
    "joker": "^0.9.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
